<h5>Listing user_compiles</h5>

<table class="table">
  <tr>
    <th>User</th>
    <th>Content</th>
    <th>Language</th>
    <th>Compiler</th>
    <th>Status</th>
    <th>Dependency</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_compiles.each do |user_compile| %>
  <tr>
    <td><%= user_compile.user_id %></td>
    <td><%= user_compile.content_id %></td>
    <td><%= user_compile.language_id %></td>
    <td><%= user_compile.compiler_id %></td>
    <td><%= user_compile.status %></td>
    <td><%= user_compile.dependency %></td>
    <td><%= link_to 'Show', user_compile %></td>
    <td><%= link_to 'Edit', edit_user_compile_path(user_compile) %></td>
    <td><%= link_to 'Destroy', user_compile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

