<div class="main" style="width:350px">
  <center><p class="lead m-null" style="font-size:50px;">Join AlgoLint</p></center>  
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
  <div><%= f.label :email, :class => "m-null"  %><br />
    <%= f.email_field :email, :autofocus => true, :class => "form-control" %></div>
  <div class="sp10"></div>
  
  <div><%= f.label :password, :class => "m-null"  %><br />
    <%= f.password_field :password, :autocomplete => "off", :class => "form-control" %></div>
  <div class="sp10"></div>
  
  <div><%= f.label :password_confirmation, :class => "m-null"  %><br />
    <%= f.password_field :password_confirmation, :autocomplete => "off", :class => "form-control" %></div>
  <div class="sp10"></div>
  <div class="sp10"></div>
  <center>
    <%= f.submit "Sign up", :class => "btn btn-lg btn-info" %>
    &nbsp;&nbsp;<a href="<%= login_user_url %>">Login</a>
  </center>
<% end %>
</div>
<div class="sp50"></div>
<div class="sp30"></div>
